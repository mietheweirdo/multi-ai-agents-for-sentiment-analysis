# .env.a2a_example
# A2A LangGraph Multi-Agent System Configuration
# Copy this to .env and update with your values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# A2A Agent Ports
QUALITY_AGENT_PORT=8001
EXPERIENCE_AGENT_PORT=8002
USER_EXPERIENCE_AGENT_PORT=8003
BUSINESS_AGENT_PORT=8004
TECHNICAL_AGENT_PORT=8005
COORDINATOR_AGENT_PORT=8000
LANGGRAPH_COORDINATOR_PORT=8010

# Logging Configuration
LOG_LEVEL=INFO

# LangGraph Default Configuration
DEFAULT_MAX_DISCUSSION_ROUNDS=2
DEFAULT_DISAGREEMENT_THRESHOLD=0.6
DEFAULT_MAX_TOKENS_PER_AGENT=150
DEFAULT_MAX_TOKENS_MASTER=500
DEFAULT_MAX_TOKENS_ADVISOR=600

# Data Pipeline Configuration (Optional)
ENABLE_REAL_SCRAPING=true
SCRAPING_TIMEOUT_SECONDS=30
MAX_ITEMS_PER_SOURCE=5
